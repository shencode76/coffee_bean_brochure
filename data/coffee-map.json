{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "data": {
      "url": "https://vega.github.io/vega-datasets/data/world-110m.json",
      "format": {
        "type": "topojson",
        "feature": "countries"
      }
    },
    "mark": "geoshape",
    "encoding": {
      "color": {
        "field": "region",
        "type": "nominal",
        "scale": {
          "scheme": "brownbluegreen"
        }
      }
    }
  }
  